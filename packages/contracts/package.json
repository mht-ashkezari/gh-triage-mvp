{
    "name": "@ghtriage/contracts",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./bff": {
            "types": "./dist/bff.contract.d.ts",
            "import": "./dist/bff.contract.js",
            "default": "./dist/bff.contract.js"
        },
        "./runs": {
            "types": "./dist/runs.contract.d.ts",
            "import": "./dist/runs.contract.js",
            "default": "./dist/runs.contract.js"
        },
        "./jsonschema": {
            "import": "./dist/jsonschema.generate.js",
            "default": "./dist/jsonschema.generate.js"
        },
        "./openapi": {
            "import": "./dist/openapi.generate.js",
            "default": "./dist/openapi.generate.js"
        },
        "./github": {
            "types": "./dist/github.d.ts",
            "import": "./dist/github.js",
            "default": "./dist/github.js"
        }
    },
    "scripts": {
        "build": "tsc -b",
        "clean": "rimraf dist tsconfig.tsbuildinfo",
        "openapi": "tsx src/openapi.generate.ts",
        "jsonschema": "tsx src/jsonschema.generate.ts"
    },
    "dependencies": {
        "@ghtriage/schemas": "workspace:*",
        "@ts-rest/core": "^3.52.1"
    },
    "devDependencies": {
        "typescript": "^5.6.3",
        "rimraf": "^6.0.1"
    },
    "turbo": {
        "dependsOn": [
            "@ghtriage/schemas#build"
        ],
        "outputs": [
            "dist/**"
        ]
    },
    "typesVersions": {
        "*": {
            "bff": [
                "dist/bff.contract.d.ts"
            ],
            "runs": [
                "dist/runs.contract.d.ts"
            ],
            "github": [
                "dist/github.d.ts"
            ],
            "*": [
                "dist/index.d.ts"
            ]
        }
    }
}