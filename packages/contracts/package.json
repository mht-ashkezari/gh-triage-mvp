{
    "name": "@ghtriage/contracts",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./bff": {
            "types": "./dist/bff.contract.d.ts",
            "import": "./dist/bff.contract.js",
            "default": "./dist/bff.contract.js"
        },
        "./runs": {
            "types": "./dist/runs.contract.d.ts",
            "import": "./dist/runs.contract.js",
            "default": "./dist/runs.contract.js"
        }
    },
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "clean": "rimraf dist",
        "openapi": "tsx src/openapi.generate.ts",
        "jsonschema": "tsx src/jsonschema.generate.ts"
    },
    "dependencies": {
        "@ts-rest/core": "^3.52.1"
    },
    "devDependencies": {
        "typescript": "^5.6.3",
        "rimraf": "^6.0.1"
    },
    "turbo": {
        "dependsOn": [
            "^build"
        ]
    }
}