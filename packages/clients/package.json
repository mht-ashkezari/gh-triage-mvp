{
    "name": "@ghtriage/clients",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./bff": {
            "types": "./dist/bff.client.d.ts",
            "import": "./dist/bff.client.js",
            "default": "./dist/bff.client.js"
        },
        "./runs": {
            "types": "./dist/runs.client.d.ts",
            "import": "./dist/runs.client.js",
            "default": "./dist/runs.client.js"
        }
    },
    "scripts": {
        "build": "tsc -b",
        "clean": "rimraf dist tsconfig.tsbuildinfo"
    },
    "dependencies": {
        "@ts-rest/core": "^3.52.1",
        "@ghtriage/contracts": "workspace:*"
    },
    "devDependencies": {
        "typescript": "^5.6.3",
        "rimraf": "^6.0.1"
    }
}