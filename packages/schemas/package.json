{
    "name": "@ghtriage/schemas",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./datasets/snapshot": {
            "import": "./dist/datasets/snapshot.js",
            "types": "./dist/datasets/snapshot.d.ts"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.js"
        }
    },
    "files": [
        "dist"
    ],
    "sideEffects": false,
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "build:watch": "tsc -p tsconfig.json -w",
        "clean": "rimraf dist"
    },
    "dependencies": {
        "zod": "^3.25.0"
    },
    "devDependencies": {
        "typescript": "^5.6.3",
        "@types/node": "^20.12.12",
        "rimraf": "^6.0.1"
    }
}