{
    "name": "@ghtriage/schemas",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./datasets/snapshot": {
            "types": "./dist/datasets/snapshot.d.ts",
            "import": "./dist/datasets/snapshot.js",
            "default": "./dist/datasets/snapshot.js"
        }
    },
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "clean": "rimraf dist"
    },
    "devDependencies": {
        "typescript": "^5.6.3",
        "rimraf": "^6.0.1"
    },
    "turbo": {
        "outputs": [
            "dist/**"
        ]
    }
}