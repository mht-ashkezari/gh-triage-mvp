{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**",
                ".next/**"
            ]
        },
        "test": {
            "dependsOn": [
                "build"
            ],
            "outputs": [
                ".coverage/**"
            ]
        },
        "lint": {},
        "typecheck": {}
    }
}